{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": "off",
        "quotes": "off",
        "no-multiple-empty-lines": [2, {"max": 2}]
    }
}
